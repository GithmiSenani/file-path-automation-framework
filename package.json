    ]
  },
  "dependencies": {
    "playwright": "^1.57.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.9",
    "open-cli": "^8.0.0",
    "pkg": "^5.8.1"
  },
  "keywords": [
    "playwright",
    "automation",
    "search"
  ],
