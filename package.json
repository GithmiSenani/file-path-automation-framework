{
  "name": "playwright-vscode-automation",
  "version": "1.0.0",
  "description": "A tool to automate searching for process information.",
  "main": "search-ui.js",
  "bin": "search-ui.js",
  "scripts": {
    "test": "playwright test",
    "start": "node search-ui.js",
    "ui": "node search-ui.js",
    "automate-fast": "node automate-search-fast.js",
